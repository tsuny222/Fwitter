<div class="form_wrap">
    <h2 class="form_head txtl">アカウント作成</h2>
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "devise/shared/error_messages", resource: resource, class:"error_messages"%>
 <form class="profile_form">
     <label for="name" class="form_label gray">名前</label>
    <%= f.text_field :name, autofocus: true, autocomplete: "name", class:"form_area"%>

     <label for="email" class="form_label gray">メールアドレス</label>
    <%= f.email_field :email, autofocus: true, autocomplete: "email", class:"form_area" %>

     <label for="password" class="form_label gray">パスワード</label>
    <% if @minimum_password_length %>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "new-password", class:"form_area" %>

     <label for="password" class="form_label gray">パスワード再入力</label>
    <%= f.password_field :password_confirmation, autocomplete: "new-password", class:"form_area" %>


    <%= f.submit "アカウント作成", class:"btnm btnpink txts" %>

<% end %>
</div>